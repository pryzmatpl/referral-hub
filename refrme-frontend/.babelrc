{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 11"]
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@vue/babel-plugin-jsx"
  ],
  "env": {
    "test": {
      "presets": ["@babel/preset-env"],
      "plugins": [
        "@vue/babel-plugin-jsx",
        "@babel/plugin-transform-modules-commonjs"
      ]
    }
  }
}