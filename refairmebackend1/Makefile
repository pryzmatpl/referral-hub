clean: stopall cleanvolumes pruneall

up:
	docker-compose -f docker-compose.yml build && docker-compose -f docker-compose.yml up -d

stopall:
	docker-compose down --remove-orphans -v

cleanvolumes:
	docker volume prune -f

pruneall:
	docker system prune --all -f

